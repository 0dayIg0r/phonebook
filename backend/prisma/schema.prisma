// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Persons {
  id String @id @default(uuid())
  name String
  phone String
  phones Phone[]

}

model Phone{
  id String @id @default(uuid())
  number String 
  owner Persons @relation(fields: [personId], references: [id])

  personId String
}